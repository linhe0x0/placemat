    "request": "2.81.0",
    "update-notifier": "2.2.0"
  },
  "devDependencies": {
    "cz-conventional-changelog": "2.0.0"
  },
  "keywords": [
    "placemat"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sqrthree/placemat.git"
  },
  "bugs": {
